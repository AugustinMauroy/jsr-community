{
  "name": "@jsr-community/rolldown-plugin-jsr-self-types",
  "version": "0.0.1-beta.1",
  "description": "A plugin for JSR to add self types to the Rolldown plugin.",
  "main": "dist/mod.js",
  "type": "module",
  "scripts": {
    "build": "rolldown build --config ./rolldown.config.ts",
    "test": "node --test './**/*.test.ts'",
    "test:coverage": "node --test --experimental-test-coverage --test-coverage-include='src/**/*' --test-coverage-exclude='**/*.test.ts' './**/*.test.ts'"
  },
  "devDependencies": {
    "rolldown": "^1.0.0-beta.9",
    "rolldown-plugin-dts": "^0.13.4",
    "typescript": "^5.8.3"
  }
}
